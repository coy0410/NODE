        <!-- Js -->
        <link href="/css/style.css" type="text/css" rel="stylesheet" />
</head>

<body>
    <!-- Header -->
    <%- include('./major_pub/header.ejs')%>
        <!-- ArtBox -->
        <div style="padding-left: 1050px; position:absolute;">
            <img src="/images/nn.png" width="300">
        </div>
        <div class="container">
            <div class="row">
                <div class="artbox col-sm-8">
                    <div class="about" style="margin-bottom: 40px;color:rgba(255, 255, 255, 0.979);">
