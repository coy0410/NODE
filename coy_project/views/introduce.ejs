                            <div><span style="font-size: 18pt;">
                            <%=o.title %>
                        </span></div>
                            <div>
                                <%=o.content %>
                            </div>
                            <div>
                                <p>&nbsp;</p>
                            </div>
                            <% } %>
                    </div>
                </div>
            </div>
        </div>
        <!-- Footer -->
        <%- include('./major_pub/footer.ejs')%>
            <!-- 弹出层 -->
            <%- include('./major_pub/popup.ejs')%>
                <script src="/js/GeeYo.js"></script>
                <script src="/js/gb2big5.js"></script>
                <script src="/js/intro.js"></script>
</body>

</html>